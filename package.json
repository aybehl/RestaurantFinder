{
  "name": "restaurantfinder",
  "version": "1.0.0",
  "description": "A repository that contains Yelp and Google Map APIs to find restaurants",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:css\"",
    "dev:server": "nodemon ./server.js",
    "dev:css": "postcss public/css/styles.css -o public/css/tailwind-built.css --watch",
    "build:css": "postcss public/css/styles.css -o public/css/tailwind-built.css"
  },
  "author": "Ayushi Behl",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.39",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.6"
  }
}
